<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush" Color="#1F1F1F" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush" Color="#F1F1F1" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
    <Color x:Key="BrandColor">#1592e5</Color>
    <SolidColorBrush x:Key="BrandBrush" Color="{StaticResource BrandColor}" />
	<SolidColorBrush x:Key="NavigationViewContentBackground" Color="Transparent" />
	<StaticResource x:Key="SystemAccentColor" ResourceKey="BrandColor" />
</ResourceDictionary>
