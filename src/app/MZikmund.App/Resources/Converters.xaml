<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:cconverters="using:CommunityToolkit.WinUI.Converters"
    xmlns:converters="using:MZikmund.Converters">
    <converters:ItemClickEventArgsConverter x:Key="ItemClickEventArgsConverter" />
    <converters:NullToVisibilityConverter x:Key="NullToVisibilityConverter" />
    <converters:InverseNullToVisibilityConverter x:Key="InverseNullToVisibilityConverter" />
    <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    <converters:EnumToIntConverter x:Key="EnumToIntConverter" />
    <converters:EmptyCollectionToVisibilityConverter x:Key="EmptyCollectionToVisibilityConverter" />
    <converters:EmptyCollectionToVisibilityConverter x:Key="InverseEmptyCollectionToVisibilityConverter" Invert="True" />
    <cconverters:BoolToVisibilityConverter
        x:Key="InverseBoolToVisibilityConverter"
        FalseValue="Visible"
        TrueValue="Collapsed" />
</ResourceDictionary>
