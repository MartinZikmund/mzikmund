		"noEmitOnError": true,
		"removeComments": false,
		"target": "es6",
		"outFile": "wwwroot/js/site.js",
		"module": "system",
		"strict": true,
	},
	"include": [
		"Scripts/**/*"
	],
	"exclude": [
		"node_modules",
		"wwwroot"
	]
}
